<Window x:Class="ProjekatPop.EditAvioKompanija"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjekatPop"
        mc:Ignorable="d"
        Title="EditAvioKompanija" Height="235.433" Width="364.567">
    <Grid>
        <Label x:Name="label1" Content="Naziv Avio Kompanije:" HorizontalAlignment="Left" Margin="8,55,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxNaziv" HorizontalAlignment="Left" Height="23" Margin="141,58,0,0" TextWrapping="Wrap" Text="{Binding Path=Naziv}" VerticalAlignment="Top" Width="187"/>
        <Button x:Name="buttonSacuvaj" Content="Sacuvaj" HorizontalAlignment="Left" Margin="42,135,0,0" VerticalAlignment="Top" Width="94" Height="28" Click="buttonSacuvaj_Click"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="200,135,0,0" VerticalAlignment="Top" Width="94" Height="28" Click="buttonCancel_Click"/>

    </Grid>
</Window>
